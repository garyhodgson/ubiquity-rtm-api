body {
	width: 800px;
  margin: 6px;
  padding: 0px;
  font-family: Verdana, sans-serif;
  font-size: 10pt;
  background-color: #ffffff;
  margin: 0px auto 0px auto;
}

.container {
	width : 800px;
	margin: 0px auto 0px auto;
}	

p {
  margin: 0px;
}

img {
  -moz-force-broken-image-icon: 1;
}

.title {
	TEXT-ALIGN:center; PADDING-TOP:1em; PADDING-BOTTOM:1em; PADDING-RIGHT:0px; PADDING-LEFT:0px
}

ul#command_summary {
	color:black;
	font-weight: bold;
}

#command_summary a {
	text-decoration:none;
	color: black;		
}

#command_summary .parameter {
	color: blue;
}

h3 a .parameter {
	color: blue;
}
ul.parameter {
	color: blue;
}

h6 { font-size: 8pt }
h5 { font-size: 8pt }
h4 { font-size: 10pt }
h3 { font-size: 12pt }
h2 { font-size: 14pt }
h1 { font-size: 18pt }

blockquote {padding: 10px; border: 1px #DDD dashed }

a img {border: 0}

.highlight {
	background-color: yellow;
}

blockquote {
	MARGIN:0 0 0 40px; BORDER:none; PADDING:0px
}

.release_notes li {
	padding: 2px 3px 3px 3px;	
}

.known_issues li {
	padding: 2px 3px 3px 3px;	
}

.completed {
	text-decoration:line-through;
}

div #known_issues {
	padding:0 5px 15px 5px;
	border: red solid 1px;
}

div #messages {
	padding:0  20px 20px 20px;
	border: lightblue solid 1px;
}

/* Table of contents */
div.toc {
  background-color: #f3f3f3;
  padding: 5px;
  border: 1px solid #ccc;
}
.toc a{
color:black;
}
.toc > ol {
  padding-left: 3em;
  font-weight: bold;
}
.toc-none {
  list-style-type: none;
}


ol.toc-subheading {
  padding-left: 1em;
  font-weight: normal;
}

.rtm_img {
	border: 1px gray solid;
	padding: 10px;
	margin: 10px;
}