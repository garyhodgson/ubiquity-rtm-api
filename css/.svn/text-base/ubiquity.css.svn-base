body { 	
	font-family:Trebuchet MS;
	font-size : 100%;
	background-color :White;
	text-align: center;
	height: auto;
	min-height: 100%;
	padding: 0;
	margin:20px auto 0px auto
	
}
	
.container {
	width : 800px;
	margin: 0px auto 0px auto;
}	

a {
	color: black;
}

a img {
	border: 0px none;
}

#title {
	padding: 20px 0 20px 0;
}



#title #feedback,#back,#home { 	
	font-size : 65%;
	padding: 0 20px 0 20px;
}

.info {
  width : 800px;
  background-color: #E7E7FF;
  margin: 5px auto 5px auto;
  text-align : left;
  font-size : 80%;
  border: 1px solid black;
  padding: 10px;
}

.info .info-title{
	text-align: right;
}

.wip a {
	font-size : 70%;
	background-color: yellow;
}

#tests .x {
	text-decoration:line-through;
}

.fade {
	color: Grey;
}

#tests h1, h2{
	margin: 0 0 0 0;
}

#tests h3{
	margin: 10px 0 5px 0;
	text-decoration:underline;
}

#cmdList {
  width : 600px;
  margin: 0px auto 0px auto;
  text-align : left;
  font-size : 90%;
}

#cmdList #cmddesc{
	font-weight: bolder;
}

#cmdList td {
  vertical-align: top;
  padding: 0 5px 5px 5px;
}

#releaseinfo {
  width : 600px;
  background-color: #E7E7FF;
  margin: 15px auto 5px auto;
  text-align : left;
  font-size : 80%;
  border: 1px solid black;
  padding: 10px;
}

#releaseinfo h1{
  margin: 0;
  padding: 0px;
  font-size : 120%;
}

.releasenumber {
  font-weight: bolder;
  text-align: right;
  vertical-align: top;
}

#knownissues {
  font-weight: bolder;
}

#features {
  font-weight: bolder;
}

#releaseinfo td {
  vertical-align: top;
}

#credit { 	
	font-size : 70%;
}

